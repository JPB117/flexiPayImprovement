<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d='urn:import:com.workpoint.icpak.client.ui.component'>

	<ui:style>
	.center {
		text-align: center;
	}
	</ui:style>

	<g:HTMLPanel styleName="tr" ui:field="row">
		<g:HTMLPanel styleName="td" ui:field="divBookingDate">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divProformaLink">
			<d:ActionLink ui:field="aProforma">Proforma</d:ActionLink>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divErnNo">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divSponsorNames">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divContactName">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divSponsorEmail">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td center" ui:field="divIsMember">
			<span ui:field="spnIsMember"></span>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divMemberNo">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divDelegateNames">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divAccomodation">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td center" ui:field="divPaymentStatus">
			<span class="label label-success" ui:field="spnPaymentStatus"></span>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td center" ui:field="divAttendance">
			<span class="label label-danger" ui:field="spnAttendance"></span>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td hide" ui:field="divAction">
			<div class="dropdown">
				<a class="btn btn-primary btn-fill dropdown-toggle" id="dropdownMenu1"
					data-toggle="dropdown" aria-expanded="true">
					-Select-
					<span class="caret"></span>
				</a>
				<ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
					<li>
						<d:ActionLink ui:field="aEnrol">Enrol in LMS</d:ActionLink>
					</li>
					<li>
						<d:ActionLink ui:field="aResendProforma">Resend Proforma</d:ActionLink>
					</li>
					<li>
						<d:ActionLink ui:field="aUpdatePayment">Update Payment</d:ActionLink>
					</li>
					<li>
						<d:ActionLink ui:field="aAttended">Mark as Attended</d:ActionLink>
					</li>
					<li>
						<d:ActionLink ui:field="aNotAttended">Mark as Not-Attended</d:ActionLink>
					</li>
					<li>
						<d:ActionLink ui:field="aEditBooking">Edit Booking</d:ActionLink>
					</li>
					<li>
						<d:ActionLink ui:field="aCancelBooking">Cancel Booking</d:ActionLink>
					</li>
				</ul>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 