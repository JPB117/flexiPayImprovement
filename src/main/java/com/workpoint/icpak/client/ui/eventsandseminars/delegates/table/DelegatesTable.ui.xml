<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">

	<ui:style>
	.overflow-x {
		overflow-x: auto;
	}
	</ui:style>
	<g:HTMLPanel styleName="tabbable {style.overflow-x}">
		<d:PagingTable ui:field="tblView" striped="false"
			isGrid="false" bordered="true" autoNumber="true" styleName="overflow-x"
			searchSectionVisible="true" searchFieldVisible="true">
		</d:PagingTable>
	</g:HTMLPanel>
</ui:UiBinder>