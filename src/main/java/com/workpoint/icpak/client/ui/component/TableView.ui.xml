<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">

	<g:HTMLPanel ui:field="overalContainer">
		<div class="search-bar">
			<div class="input-append">
				<d:TextField placeholder="Search here" ui:field="txtSearch"></d:TextField>
			</div>
		</div>
		<g:HTMLPanel styleName="table table-striped table-hover table-bordered"
			ui:field="tblContainer">
			<div class='thead'>
				<g:HTMLPanel styleName="tr" ui:field="panelHeader">
				</g:HTMLPanel>
			</div>

			<g:FlowPanel ui:field="panelBody" styleName='tbody'>
			</g:FlowPanel>

			<g:HTMLPanel ui:field="panelFooter" styleName='tbody'>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>