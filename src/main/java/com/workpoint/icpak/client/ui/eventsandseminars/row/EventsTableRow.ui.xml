<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">
	<ui:style>
		.active{
		   background-color:#99FF33;
		}
		.in-active{
		   background-color:#00FFFF;
		}
	</ui:style>
	<g:HTMLPanel styleName="tr active" ui:field="row">
		<g:HTMLPanel styleName="td" ui:field="divDate">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divEventName">
			<g:Anchor ui:field="aEventName"></g:Anchor>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divEventType">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divEventLocation">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divCPDHours">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divDelegates">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divPaidAmount">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divUnPaidAmount">
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 