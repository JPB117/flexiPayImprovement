<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">

	<g:HTMLPanel styleName="tr" ui:field="row">
		<g:HTMLPanel styleName="td" ui:field="divDate">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divSubmissionDate">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divMemberName">
			<d:ActionLink ui:field="aMemberName"></d:ActionLink>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divEmail">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divPaymentStatus">
			<span ui:field="spnPaymentStatus"></span>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divApplicationStatus">
			<span ui:field="spnApplicationStatus"></span>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td hide" ui:field="divAction">
			<d:ActionLink styleName="fa fa-check btn btn-primary"></d:ActionLink>
			<d:ActionLink styleName="fa fa-times btn btn-danger"></d:ActionLink>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 