<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	xmlns:d='urn:import:com.workpoint.icpak.client.ui.component' xmlns:up='urn:import:com.workpoint.icpak.client.ui.upload.custom'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:t='urn:import:com.workpoint.icpak.client.ui.component.tabs'
	ui:generateLocales='default'>

	<g:HTMLPanel ui:field="container"
		styleName="content-wrapper ng-scope ng-fadeInUp">
		<h3 class="ng-scope">
			User Profile
		</h3>

		<div class="row">
			<div class="col-xs-12">
				<div class="row">
					<div class="col-xs-2">
						<img class="thumbnail"
							src="http://solutech.co.ke/icpak/assets/img/no-image.png" width="100px" />
						<a href="">Edit Image</a>
					</div>
					<div class="col-xs-6">
						<h5>
							<strong>Tom Kimani Muriranja</strong>
						</h5>
						<h5>
							mdkimani@gmail.com
						</h5>
						<d:ActionLink ui:field="aEdit">Edit
							Profile</d:ActionLink>
						|
						<d:ActionLink ui:field="aChangePassword">Change
							Password</d:ActionLink>
						<br />
						<span>
							<h5>
								<span class="label label-info">Practising-Member</span>
							</h5>
						</span>
						<div class="progress warning">
							<div class="progress-bar " role="progressbar" aria-valuenow="60"
								aria-valuemin="0" aria-valuemax="100" style="width: 30%;">60%
								Complete</div>
						</div>
					</div>

				</div>
				<div class="row">
					<div class="panel panel-default">
						<div class="panel-heading">Profile Information</div>

						<div class="panel-body">
							<div class="col-xs-12">
								<!-- Tabs -->
								<t:TabPanel ui:field="divTabs">
								</t:TabPanel>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
