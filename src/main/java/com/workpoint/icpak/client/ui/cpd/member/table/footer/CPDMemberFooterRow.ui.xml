<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">

	<ui:style>
	.summary {
		background-color: #F5F5F5;
	}
	
	.right {
		text-align: right;
	}
	
	.group {
		display: table-row-group;
	}
	
	.row {
		display: table-row;
	}
	
	.cell {
		display: table-cell;
		width: 100%;
		border: 1px solid grey;
	}
	</ui:style>
	<g:HTMLPanel styleName="tr {style.summary}" ui:field="row">
		<g:HTMLPanel styleName="td" ui:field="divCount">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divCourseName">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divStartDate">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divCPDStatus">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divAction">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td {style.right}" ui:field="divDesc">
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divCPD">
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 