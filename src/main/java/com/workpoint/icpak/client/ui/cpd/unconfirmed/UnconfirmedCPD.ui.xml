	xmlns:record='urn:import:com.workpoint.icpak.client.ui.cpd.record'
	xmlns:table='urn:import:com.workpoint.icpak.client.ui.cpd.table'>
	<g:HTMLPanel>
		<div class="row">

			<g:HTMLPanel ui:field="panelListing">
				<div class="col-lg-12">
					<d:ActionLink ui:field="aCreate" styleName="btn btn-primary btn-fill">
						<span class="fa fa-plus"></span>
						Record CPD
					</d:ActionLink>
				</div>
				<br />
				<g:HTMLPanel styleName="col-lg-12">
					<table:CPDTable ui:field="tblView">
					</table:CPDTable>
				</g:HTMLPanel>
			</g:HTMLPanel>

			<g:HTMLPanel styleName="col-lg-12" ui:field="panelNew">
				<record:RecordCPD></record:RecordCPD>
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 