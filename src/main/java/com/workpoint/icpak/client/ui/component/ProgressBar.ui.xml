<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel styleName="progress progress-striped progress-xs">
			<div ui:field="divProgress" role="progressbar" aria-valuenow="0" aria-valuemin="0"
				aria-valuemax="100" class="progress-bar progress-bar-green">
				<span ui:field="spnCompletion" class="text-muted small"></span>
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 