<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:d='urn:import:com.workpoint.icpak.client.ui.component'
	ui:generateLocales='default'>

	<ui:style>
	.success-message {
		width: initial;
		position: absolute;
		left: 40%;
		height: 40px;
		top: 5px;
		padding-top: 8px;
	}
	</ui:style>

	<g:HTMLPanel ui:field="divNavbar">
		<header class="topnavbar-wrapper ng-scope">
			<!-- START Top Navbar -->
			<nav role="navigation" class="navbar topnavbar ng-scope">
				<div class="navbar-header">
					<d:ActionLink styleName="navbar-toggle collapsed"
						ui:field="aToggleSideBar">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</d:ActionLink>

					<a href="#/" class="navbar-brand">
						<div class="brand-logo">
							<img class="img-responsive" src="img/LogoWhitebg.png" ui:field="imgLogo"/>
						</div>
					</a>
				</div>
				<div class="nav-wrapper">
					<div
						class="alert alert-success {style.success-message} hide alert-dismissible"
						role="alert" ui:field="divAlert">
						<button type="button" class="close" data-dismiss="alert"
							aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
						<span ui:field="spnAlertMessage">
							<span class="fa fa-ok"></span>
							Successfully Saved!
						</span>
					</div>

					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="collapse navbar-collapse">
						<!-- START Right Navbar -->
						<ul class="nav navbar-nav navbar-right">
							<li>
								<a href="#profile">
									<em class="fa fa-user"></em>
									<span ui:field="spnLoggedInUser"></span>
								</a>
							</li>
							<li>
								<g:Anchor ui:field="aLogout">
									<em class="fa fa-power-off"></em>
									LogOut
								</g:Anchor>
							</li>
							<!-- END Contacts menu -->
						</ul>
						<!-- END Right Navbar -->
					</div>
				</div>
			</nav>
			<!-- END Top Navbar -->
		</header>
	</g:HTMLPanel>
</ui:UiBinder>
