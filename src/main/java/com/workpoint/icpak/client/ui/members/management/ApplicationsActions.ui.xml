	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div class="form-group">
			<div class="row">
				<div class="col-xs-6">
					<d:DropDownList ui:field="lstApplicationStatus"
						styleName="form-control"></d:DropDownList>
				</div>
				<div class="col-xs-6">
					<d:DropDownList ui:field="lstPaymentStatus"
						styleName="form-control"></d:DropDownList>
				</div>
			</div>
		</div>

		<div class="form-group" ui:field="divMgtComment">
			<div class="row">
				<div class="col-xs-12">
					<label for="from">Comment:</label>
					<d:TextArea ui:field="txtMgmtComment" styleName="form-control"
						rows="5"></d:TextArea>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 