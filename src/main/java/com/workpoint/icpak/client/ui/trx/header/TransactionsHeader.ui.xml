<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:d='urn:import:com.workpoint.icpak.client.ui.component'>

	<g:HTMLPanel styleName="summary-container">
		<div class="row-fluid">
			<div class="span4">
				<div class="big-text">
					<span class="" ui:field="spnMoneyIn" title="Money In">
					15,000/-
					</span>
				</div>
				<div class="bold muted helper-font-small">MONEY IN</div>
			</div>

			<div class="span4">
				<div class="big-text">
					<span class="" ui:field="spnMoneyOut" title="Money Out">
					14,000/-
					</span>
				</div>
				<div class="bold muted helper-font-small">MONEY USED</div>
			</div>

			<div class="span4 no-border">
				<div class="big-text">
					<span class="" ui:field="spnBalance" title="Balance">
					1,000/-
					</span>
				</div>
				<div class="bold muted helper-font-small">BALANCE</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 