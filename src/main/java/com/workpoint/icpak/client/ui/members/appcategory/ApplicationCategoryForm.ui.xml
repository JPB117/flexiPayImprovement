<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div class="form-group">
			<div class="row">
				<div class="col-xs-12">
					<d:IssuesPanel ui:field="issuesPanel" styleName="hide alert alert-danger"></d:IssuesPanel>
				</div>
			</div>
		</div>
		<div class="form-group">
			<div class="row">
				<div class="col-xs-12">
					<d:DropDownList ui:field="lstApplicationCategories"
						styleName="form-control"></d:DropDownList>
				</div>
			</div>
		</div>
		<div class="form-group">
			<div class="row">
				<div class="col-xs-12">
					<label for="from">Application Amount:</label>
					<d:TextField ui:field="txtApplicationAmount" styleName="form-control"
						type="number"></d:TextField>
				</div>
			</div>
		</div>

		<div class="form-group">
			<div class="row">
				<div class="col-xs-12">
					<label for="from">Renewal Amount:</label>
					<d:TextField ui:field="txtRenewalAmount" styleName="form-control"
						type="number"></d:TextField>
				</div>
			</div>
		</div>

		<div class="form-group" ui:field="divMgtComment">
			<div class="row">
				<div class="col-xs-12">
					<label for="from">Description:</label>
					<d:TextArea ui:field="txtDescription" styleName="form-control"
						rows="5"></d:TextArea>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 