<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:d='urn:import:com.workpoint.icpak.client.ui.component'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="row">
		
		<div class="col-sm-12">
			<h4 class="info-text">
				<span ui:field="spnSelected">Fill In the Delegates to Attend:</span>
			</h4>
			<hr />
		</div>
		
		<div class="col-sm-12">
			<d:ActionLink ui:field="aAddRow" styleName="btn btn-primary">Add Delegate</d:ActionLink>
		</div>

		<div class="col-sm-12">
			<d:TableView ui:field="tblDelegates"></d:TableView>
		</div>

	</g:HTMLPanel>

</ui:UiBinder>