<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">

	<g:HTMLPanel ui:field="container" styleName="col-lg-12">
		<div class="col-lg-12">
			<d:BulletListPanel ui:field="ankaPanel" styleName="pagination text-center"/>
		</div>

		<div class="col-lg-4">
			<p class="text-muted">
				Displaying
				<span ui:field="spnOffset" />
				-
				<span ui:field="spnPageEnd" />
				of
				<span ui:field="spnTotal" />
			</p>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 