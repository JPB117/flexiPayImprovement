<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.workpoint.icpak.client.ui.component">

	<g:HTMLPanel styleName="tr" ui:field="row">
		<g:HTMLPanel styleName="td" ui:field="divFirmName">
		    <div>
		        <p>
		            <strong><g:Label ui:field="lFirmName"/></strong>
		        </p>
		        <p>
		            <g:Label ui:field="lPartners"/>
		        </p>
		    </div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divAddress">
		    <div>
		        <p>
		            <g:Label ui:field="lAddress1"/>
		        </p>
		        <p>
		            <g:Label ui:field="lAddress2"/>
		        </p>
		        <p>
		            <g:Label ui:field="lAddress3"/>
		        </p>
		    </div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divContacts">
		    <div>
		        <p>
		            Telephone:<g:Label ui:field="lTelephone"/>
		        </p>
		        <p>
		            Email:<g:Label ui:field="lEmail"/>
		        </p>
		    </div>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 